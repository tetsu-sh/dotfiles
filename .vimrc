set number 
set list
set title
set ruler
syntax on
set incsearch
set hlsearch
set smartindent
set wildmenu
set laststatus=2
set ignorecase
set clipboard=unnamed

set expandtab
set shiftwidth=0
set splitbelow
set tabstop=4
set termwinsize=15x0
filetype plugin on
let g:netrw_banner=0
let g:netrw_browse_split=4
let g:netrw_liststyle=3
let g:netrw_preview=1
let g:netrw_winsize=75
